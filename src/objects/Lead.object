    <webLinks>
        <fullName>Daddy_Analytics_Update</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Daddy Analytics - Update</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}

// alert user if Daddy Analytics custom settings API Key or Site ID are not populated
var result = sforce.connection.query(&quot;Select Id From DaddyAnalytics__DaddyAnalytics__c Where DaddyAnalytics__DA_Site_ID__c = &apos;&apos; Or DaddyAnalytics__DA_API_Key__c = &apos;&apos;&quot;);
var records = result.getArray(&quot;records&quot;);

if (records[0]) {

  alert(&quot;Daddy Analytics settings must be populated before Leads can be updated.&quot;);

} else {
  
  // alert user if Daddy Analytics Token has not been populated for the Lead
  if (&quot;{!Lead.DA_Token__c}&quot; == &quot;&quot;) {

    alert(&quot;Daddy Analytics token is not populated for this Lead.&quot;);

  } else {

    // alert user if Daddy Analytics data has already been added to the Lead
    if (&quot;{!Lead.DA_User_ID__c}&quot; != &quot;&quot;) {

      alert(&quot;Daddy Analytics data has already been added to this Lead.&quot;);
  
    } else {

      // populate Lead with Daddy Analytics data
      var calloutResult = sforce.apex.execute(&quot;DaddyAnalytics/DaddyAnalyticsRetrieveData&quot;,&quot;onButtonRetrieveData&quot;,{DA_Token:&quot;{!Lead.DA_Token__c}&quot;});

      if (calloutResult == &quot;&quot;) {
        
        // alert user is no data is returned from Daddy Analytics
        alert(&quot;No data returned from Daddy Analytics. If this Lead has just been created please wait 5 minutes and try again.&quot;);

        } else {

          var url = parent.location.href;
          parent.location.href = url;
        }
    }
  }
}</url>
    </webLinks>
    <webLinks>
        <fullName>Daddy_Analytics_Update_All</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Daddy Analytics - Update All</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}

// alert user if Daddy Analytics custom settings API Key and Site ID are not populated
var result = sforce.connection.query(&quot;Select Id From DaddyAnalytics__DaddyAnalytics__c Where DaddyAnalytics__DA_Site_ID__c = &apos;&apos; Or DaddyAnalytics__DA_API_Key__c = &apos;&apos;&quot;);
var records = result.getArray(&quot;records&quot;);

if (records[0]) {

  alert(&quot;Daddy Analytics settings must be populated before Leads can be updated.&quot;);

} else {

  // run apex batch
  var batchResult = sforce.apex.execute(&quot;DaddyAnalytics/DaddyAnalyticsRetrieveDataBatch&quot;,&quot;onButtonRetrieveDataBatch&quot;,{});

  // alert user that batch process will run in the background
  alert(&quot;Your Leads are now being updated by Daddy Analytics. This process will run in the background and depending on the number of Leads should be complete in under a minute. Please refresh your browser to see the results.&quot;);

  // refresh screen
  var url = parent.location.href;
  parent.location.href = url;
}</url>
    </webLinks>
